{
  "name": "retext-overuse",
  "version": "1.1.1",
  "description": "Check words for overuse with Retext",
  "license": "MIT",
  "keywords": [
    "keyword",
    "phrase",
    "overuse",
    "synoynm",
    "duplicates",
    "duplication",
    "retext"
  ],
  "dependencies": {
    "array-differ": "^1.0.0",
    "nlcst-search": "^1.1.1",
    "nlcst-to-string": "^1.0.0",
    "object-keys": "^1.0.9",
    "quotation": "^1.0.1",
    "stopwords": "0.0.5",
    "thesaurus": "0.0.0"
  },
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dunckr/retext-overuse.git"
  },
  "author": "Duncan Beaton <dunckr@gmail.com>",
  "devDependencies": {
    "browserify": "^13.0.0",
    "eslint": "^1.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.4.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "remark": "^3.0.0",
    "remark-comment-config": "^2.0.2",
    "remark-github": "^3.0.0",
    "remark-lint": "^2.0.2",
    "remark-slug": "^3.0.1",
    "remark-validate-links": "^2.0.2",
    "retext": "^1.0.0",
    "tape": "^4.0.0",
    "vfile": "^1.2.0",
    "vfile-reporter": "^1.5.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-bundle": "browserify index.js --bare -s retextOveruse > retext-overuse.js",
    "build-mangle": "esmangle retext-overuse.js > retext-overuse.min.js",
    "build": "npm run build-md && npm run build-bundle",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "test-api": "node test.js",
    "test-coverage": "istanbul cover test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  }
}
